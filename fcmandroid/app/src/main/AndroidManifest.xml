<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.ac.kumoh.LostNFount.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="kr.ac.kumoh.LostNFount.finalproject.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="kr.ac.kumoh.LostNFount.finalproject.SignUpActivity">
        </activity>
        <activity
            android:name="kr.ac.kumoh.LostNFount.finalproject.SubActivity">
        </activity>
        <activity
            android:name="kr.ac.kumoh.LostNFount.finalproject.WriteActivity">
        </activity>
        <activity
            android:name="kr.ac.kumoh.LostNFount.finalproject.Setting">
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" /> <!-- 인터넷 접속 권한 허가 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /> <!--외부 Storage 읽기 권한 -->
</manifest>